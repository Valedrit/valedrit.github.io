{% macro xepchu(doc) %}
{% import '_functions' as func %}
{% set ubot = get_data('bot')[0].data|json_decode %}
{% set xc = ubot['xc'] %}
{% set countdown = (ubot['xc'].time)-("now"|date("U")) %}
{% set time_answer = 100-countdown %}
{% set xu = 10*countdown %}
{% set list =  {"cau1":"hoa sen","cau2":"nhà trọ","cau3":"nghỉ lễ","cau4":"nghĩa địa","cau5":"địa vị","cau6":"áo phông","cau7":"trường học","cau8":"nhà nghỉ","cau9":"siêu nhân","cau10":"biển Đông","cau11":"tờ giấy","cau12":"hôn nhân","cau13":"gia đình","cau14":"tình dục","cau15":"ngắm gái","cau16":"đẹp trai","cau17":"Thái Bình Dương","cau18":"thủ đô","cau19":"xinh đẹp","cau20":"ngôi nhà","cau21":"rau cải cúc","cau22":"thế giới ảo tình yêu thật","cau23":"thất tình","cau24":"công chúa bong bóng","cau25":"tự hào","cau26":"văn học","cau27":"hòa bình","cau28":"chiến tranh","cau29":"động vật","cau30":"chén cơm","cau31":"chè khoán","cau32":"Hoàng Văn Thụ","cau33":"lịch sử","cau34":"nhân tạo","cau35":"Võ Thị Sáu","cau36":"việt vị","cau37":"hậu môn","cau38":"phố nhỏ","cau39":"Nhật Bản","cau40":"cơ khí","cau41":"con bướm xuân","cau42":"sinh học","cau43":"học sinh","cau44":"Ngô Tất Tố","cau45":"kim loại","cau46":"phi kim","cau47":"mỏ địa chất","cau48":"con sò","cau49":"trứng gà tre","cau50":"yêu thương","cau51":"trẻ con","cau52":"con trâu","cau53":"đường nho","cau54":"đường nhựa","cau55":"nhớ nhung","cau56":"tập viết","cau57":"Nam Cao","cau58":"Xuân Diệu","cau59":"Phan Bội Châu","cau60":"cải cách","cau61":"phát triển","cau62":"Đông Nam Á","cau63":"tứ tử trình làng","cau64":"ngũ tử cướp cái"} %}
{% set rd = random(1..64) %}
{% set key = "cau"~rd %}
{%set text=func.rwurl(list[key])|replace({"-":" "})%}{%set quest=""%} 
{%for i in 0..text|length-1%} 
{%set arr = text|split('')%} 
{%set rand = random(text|length-1)%} 
{%set quest= quest~arr[rand]%} 
{%set text = (arr[:rand]|join('')~arr[(rand+1):]|join(''))%} 
{%endfor%}
{% set end = list[key] %}
{% set raw_end = func.rwurl(end)|replace({"-":" "}) %}
{% set time_start = 'now'|date('U') %}
{% set id = func.get('guestbook')|split('@')[99]|trim %}
{% if 'xep chu' in doc|lower and xc.time < time_start or 'xếp chữ' in doc|lower and xc.time < time_start or 'xc' in doc|lower and xc.time < time_start %}
{{func.add('bot','xc',{"id":xc.id|trim+1,"time":"now"|date("U")+100,"word":end,"raw_word":raw_end,"end":"no","quest":''~quest~''})}}
{{func.add('chat_'~id,'name','bot')}}
{{func.add('chat_'~id,'time','now'|date('U'))}}
{{func.add('chat_'~id,'comment','Hãy sắp xếp các chữ sau thành từ hoặc cụm từ hoàn chỉnh: [b]'~quest~'[/b]')}} 
{{func.up('guestbook',id,'up')}}
{% endif %}
{% if xc['word'] in doc|lower and xc.time >= time_start or xc['raw_word'] in doc|lower and xc.time >= time_start %}
{% if xc.time > time_start %}
{{func.add('chat_'~id,'name','bot')}}
{{func.add('chat_'~id,'time','now'|date('U'))}}
{{func.add('chat_'~id,'comment','Chúc mừng bạn đã trả lời đúng câu hỏi vừa rồi trong '~time_answer~' giây ! Bạn xếp chữ giỏi quá đi :))')}} 
{{func.up('guestbook',id,'up') }}
{{func.add('bot','xc',{"id":xc.id|trim,"time":"now"|date("U"),"word":xc.word,"raw_word":xc.raw_word,"end":"yes"})}}
{% endif %}
{% endif %}
{% endmacro %}

